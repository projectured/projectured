(in-package :projectured)

;;;;;;
;;; Document

(def document style/color ()
  ((alpha :type (integer 0 255))
   (red :type (integer 0 255))
   (green :type (integer 0 255))
   (blue :type (integer 0 255))
   (raw :type t)))

;;;;;;
;;; Construction

(def (function e) make-style/color (alpha red green blue)
  (make-instance 'style/color
                 :alpha alpha
                 :red red
                 :green green
                 :blue blue
                 :raw nil))

;;;;;;
;;; API

(def (function e) color= (color-1 color-2)
  (and (= (alpha-of color-1) (alpha-of color-2))
       (= (red-of color-1) (red-of color-2))
       (= (green-of color-1) (green-of color-2))
       (= (blue-of color-1) (blue-of color-2))))

(def (function e) color=* (color-1 color-2)
  (if (and color-1 color-2)
      (color= color-1 color-2)
      (eq color-1 color-2)))

;;;;;;
;;; Definer

(def (definer e :available-flags "e") color (name (red green blue))
  `(def special-variable ,name (make-style/color 255 ,red ,green ,blue)))

